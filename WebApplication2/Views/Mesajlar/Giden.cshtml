@using WebApplication2.Models.Entity
@model List<TBLMESAJLAR>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}



<h2>Giden Mesajlar</h2>


<table class="table table-hover">

    <tr>
        <th>ALICI</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Islem</th>
    </tr>

    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ALICI</td>
            <td>@x.KONU</td>
            <td>@x.ICERIK</td>
            <td>@x.TARIH</td>
        </tr>
    }
</table>