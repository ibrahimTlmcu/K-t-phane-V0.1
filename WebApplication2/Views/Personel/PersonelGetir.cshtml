@model  WebApplication2.Models.Entity.TBLPERSONEL
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2 style="text-align:center">Personel Guncelle</h2>



<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
@using (Html.BeginForm("PersonelGuncelle", "Personel", FormMethod.Post))
{

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <div class="form-group">
        <div style="margin-bottom:15px"></div>
        <lable> Personel ID </lable>
        @Html.TextBoxFor(y => y.ID, new { @class = "form-block" })
    </div>

    <div class="form-group">
        <div style="margin-bottom:15px"></div>
        <lable> Personel Adi </lable>
        @Html.TextBoxFor(y => y.PERSONEL, new { @class = "form-block" })
    </div>


    <div style="margin-top:15px">

        <button class="btn bg-success"> Personel Guncelle</button>
    </div>



}